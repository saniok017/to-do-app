{
  "name": "to-do-app",
  "version": "0.0.1",
  "description": "todo-app",
  "main": "index.js",
  "scripts": {
    "lint": "eslint -c .eslintrc.js ./**/*.js",
    "fix-lint": "eslint -c .eslintrc.js ./components/**/*.js --fix",
    "build": "next build",
    "start": " cross-env NODE_ENV=production node server.js",
    "test": "jest",
    "dev": "nodemon server.js"
  },
  "author": "saniok017",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "jest": "^24.1.0",
    "jest-environment-enzyme": "^7.0.1",
    "jest-enzyme": "^7.0.1",
    "nodemon": "^1.18.9",
    "path": "^0.12.7",
    "react-test-renderer": "^16.8.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@material-ui/core": "^3.9.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "body-parser": "^1.13.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "express": "^4.16.4",
    "express-partials": "^0.3.0",
    "express-session": "^1.11.3",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.11",
    "method-override": "^2.3.5",
    "next": "^7.0.2",
    "passport": ">= 0.0.0",
    "passport-github2": ">= 0.0.0",
    "prop-types": "^15.7.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-select": "^2.3.0",
    "regenerator-runtime": "^0.11.1",
    "router": "^1.1.3",
    "sw-precache-webpack-plugin": "^0.11.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saniok017/to-do-app"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "./babel.wrapper.js"
    },
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|xlsx|html)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.next/"
    ]
  }
}
